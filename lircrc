## VLC
# Start vlc keybinds
#
# Play
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = PLAY
     config = key-play
end
# Pause
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = PAUSE
     config = key-pause
end
# Stop
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = STOP
     config = key-stop
end
# Fast Foward
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = FWD
     config = key-faster
end
# Reverse
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = REW
     config = key-slower
end
# Previous 
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = REPLAY
     config = key-prev
end
# Next
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = SKIPFOWARD
     config = key-next
end
# Up
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = UP
     config = key-nav-up
end
# Down
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = Down
     config = key-nav-down
end
# Left
begin
     remote = Dell
     prog = irexec
     prog = vlc
     button = LEFT
