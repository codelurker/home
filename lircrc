# Mute
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = MUTE
     config = mute
end
# Pause
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = PAUSE
     config = pause
end
# Play
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = PLAY
     config = seek +0
end
# Stop
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = STOP
     config = quit
end
# Quit
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = BACKEXIT
     config = get_time_pos
#     config = get_property stream_pos
end
# Exit
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = BACKEXIT
     config = quit
end
# Power
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = POWER
     config = quit
end
# Next
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = SKIPFOWARD
     config = seek_chapter +1 0
end
# Replay
begin
     remote = Dell
     prog = irexec
     prog = mplayer
     button = REPLAY
     config = seek_chapter -1 0
end
