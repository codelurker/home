## Lirc Keys Using Haupauge PVR      remote
#

# Apps
##

## ALSA
# Start alsa keybinds
#
# Vol+ 3
begin
     remote = Haupauge
     prog = irexec
     button = VOL+
     config = amixer set PCM 4%+ &
     repeat = 2
end
# Vol- 3
begin
     remote = Haupauge
     prog = irexec
     button = VOL-
     config = amixer set PCM 4%- &
     repeat = 2
end
# Mute/UnMute Toggle
begin
     remote = Haupauge
     prog = irexec
     button = MUTE
     config = amixer set Master toggle &
     repeat = 0
end
##
# Video - TV
#

# Start TV-Viewer
begin
     remote = Dell
     prog = irexec
     button = TV2
     config = v4l2-ctl -i 1 && mplayer -ao alsa -vf pp=lb /dev/video0
     repeat = 2
end
begin
     remote = Dell
     prog = irexec
     button = TV
     config = v4l2-ctl -i 1 && vlc --hdtv-fix --aspect-ratio=16:9 --monitor-par=4:3 --deinterlace-mode=x --disable-screensaver --control lirc /dev/video0
     repeat = 2
end
# Start VLC
begin
     remote = Dell
     prog = irexec
     button = MCE
     config = elisa &
     repeat = 2
end

# Play/Pause Toggle Moc
begin
     remote = Apple
     prog = irexec
     button = PLAY
     config = mocp -G
     repeat = 0
end
# Stop Moc
begin
     remote = Apple
     prog = irexec
     button = MINUS
     config = amixer set PCM 5%- &
     repeat = 0
end
# Toggle Mute Moc
begin
     remote = Apple
     prog = irexec
     button = PLUS
     config = amixer set PCM 5%+ &
     repeat = 0
end
# Stop Moc
# Next Song Moc
begin
     remote = Apple
     prog = irexec
     button = FWD
     config = mocp -f
     repeat = 0
end
# Prev Song Moc
begin
     remote = Apple
     prog = irexec
     button = REW
     config = mocp -r
     repeat = 0
end
# Exit Moc
begin
     remote = Apple
     prog = irexec
     button = MENU
     config = mocp -x
     repeat = 0
end

## Xine
# Start xine keybinds
#
begin
     remote = Haupauge
     button = PLAY
     prog   = xine
     repeat = 0
     config = Play
end

# playback pause toggle
begin
     remote = Haupauge
     button = PAUSE
     prog   = xine
     repeat = 0
     config = Pause
end

# stop playback
begin
     remote = Haupauge
     button = STOP
     prog   = xine
     repeat = 0
     config = Stop
end

# take a snapshot
begin
     remote = Haupauge
     button = RED
     prog   = xine
     repeat = 0
     config = Snapshot
end

# eject the current medium
begin
     remote = Haupauge
     button = GREEN
     prog   = xine
     repeat = 0
     config = Eject
end

# select and play next MRL in the playlist
begin
     remote = Haupauge
     button = xxxxx
     prog   = xine
     repeat = 0
     config = NextMrl
end

# select and play previous MRL in the playlist
begin
     remote = Haupauge
     button = xxxxx
     prog   = xine
     repeat = 0
     config = PriorMrl
end

# loop mode toggle
begin
     remote = Haupauge
     button = xxxxx
     prog   = xine
     repeat = 0
     config = ToggleLoopMode
end

# stop playback after played stream
begin
     remote = Haupauge
     button = xxxxx
     prog   = xine
     repeat = 0
     config = PlaylistStop
end

